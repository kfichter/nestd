{
  "extends": [
    "tslint:latest",
    "tslint-config-prettier",
    "tslint-plugin-prettier"
  ],
  "rules": {
    "prettier": true,
    "no-unused-expression": false,
    "interface-name": false,
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "no-empty-interface": false,
    "no-empty": false,
    "array-type": [false],
    "member-ordering": [false],
    "variable-name": [false],
    "ban-types": false,
  }
}
